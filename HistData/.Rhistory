# Read 2007 EUR/USD Data
EURUSD <- read.table("DAT_ASCII_EURUSD_M1_2007.csv", sep=";")
# Assign column names to the table
colnames(EURUSD) <- c("Date", "Open", "High", "Low", "Close", "Volume")
# Remove Volume column from table
EURUSD <- EURUSD[,-6]
# Get the time
EURUSD$Time <- ldply(strsplit(as.character(EURUSD$Date), " "))[, 2]
#Convert Time datatype to numeric
class(EURUSD[, "Time"]) # Character
EURUSD$Time <- as.numeric(EURUSD$Time)
class(EURUSD[, "Time"]) # numeric
# Clean up Date
EURUSD$Date <- ldply(strsplit(as.character(EURUSD$Date), " "))[, 1]
# Split Date into year / month / day
EURUSD$Year <- substr(EURUSD$Date, 1, 4)
EURUSD$Month <- substr(EURUSD$Date, 5, 6)
EURUSD$Day <- substr(EURUSD$Date, 7, 8)
# Remove date column
EURUSD <- EURUSD[, -1]
# Fetch all the rows where time is between 10am to 11am
EURUSD_10AM_11AM <- subset(EURUSD, Time >= 100000 & Time <= 110000)
colnames(EURUSD_10AM_11AM) <- c("Open", "High", "Low", "Close", "Time", "Year", "Month", "Day")
# Fetch Closing Values from 10am - 11am
