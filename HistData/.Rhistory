paste(result, collapse = " ")
# Result Of Assignment 1A - calculating 5 min returns from 10 am 11am
EURUSD_FIRST_MINUTES = EURUSD_10AM_11AM[seq(1, NROW(EURUSD_10AM_11AM), by = 5),]
EURUSD_FIFTH_MINUTES = EURUSD_10AM_11AM[seq(5, NROW(EURUSD_10AM_11AM), by = 5),]
EURUSD_FIRST_MINUTES = EURUSD_FIRST_MINUTES$Close
EURUSD_FIFTH_MINUTES = EURUSD_FIFTH_MINUTES$Close
EURUSD_FIVE_MIN_RETURNS = ((EURUSD_FIFTH_MINUTES / EURUSD_FIRST_MINUTES) - 1 ) * 100
head(EURUSD_FIVE_MIN_RETURNS)
